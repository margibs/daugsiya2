		          $(this).css('height', hiddenDiv.height());

		      });

		      $('.maginterviewmain').vTicker('init', {
		      	speed: 600, 
			    padding:10
			  });
			  $('.friendsnotification').vTicker('init', {
		      	speed: 400,

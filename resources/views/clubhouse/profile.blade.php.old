		   function loadApp() {

		  // Create the flipbook

		  $('.flipbook').turn({
		      // Width

		      width:622,
		      
		      // Height

		      height:400,

		      // Elevation

		      elevation: 50,
		      
		      // Enable gradients

		      gradients: true,
		      
		      // Auto center this flipbook

		      autoCenter: true

		  });
		}

		// Load the HTML4 version if there's not CSS transform

		yepnope({
		  test : Modernizr.csstransforms,
		  yep: ['{{ asset("js/turn.min.js") }}'],
		  nope: ['{{ asset("js/turn.html4.min.js") }}'],
		  both: ['{{ asset("css/flipbook.css") }}'],
		  complete: loadApp
		});

		var 
		    menuTop = document.getElementById( 'cbp-spmenu-s3' ),
		    showTop = document.getElementById( 'showTop' ),

		    body = document.body;

		showTop.onclick = function() {
		  classie.toggle( this, 'active' );
